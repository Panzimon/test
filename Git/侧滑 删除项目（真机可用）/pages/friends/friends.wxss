.container {
    position: relative;
    padding-top: 120rpx;
}
.searchInfo {
    width: 750rpx;
    height: 120rpx;
    background-color: #C6C6CB;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100;
}
.searchMessage {
    height: 80rpx;
    margin: 20rpx 40rpx;
    padding: 0rpx 30rpx;
    border-radius: 12rpx;
    background-color: #fff;
}
.changePageMask {
    overflow: hidden;
}
.userImage {
    width: 60rpx;
    height: 60rpx;
}
.userInfo {
    height: 96rpx;
    padding: 18rpx 30rpx;
    position: relative;
    left: 0rpx;
    border-top: 2rpx solid #ccc;
}
.letter {
    height: 50rpx;
    line-height: 50rpx;
    font-size: 35rpx;
    background-color: #E0E0E0;
    padding-left: 30rpx;
}
.userName {
    height: 60rpx;
    line-height: 60rpx;
    padding-left: 30rpx;
}
.deleteFriend {
    position: absolute;
    right: -160rpx;
    top: -2rpx;
    height: 94rpx;
    line-height: 94rpx;
    border-top: 2rpx solid #ccc;
    width: 160rpx;
}